# Copy this shit to .env and fill in your actual API keys
# Mix of browser-use variables and custom uzdabrazor variables

# OPENAI
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_ENDPOINT=https://api.openai.com/v1

# ANTHROPIC  
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here
ANTHROPIC_ENDPOINT=https://api.anthropic.com

# GOOGLE
GOOGLE_API_KEY=your-google-gemini-api-key-here

# DEEPSEEK
DEEPSEEK_API_KEY=sk-your-deepseek-api-key-here
DEEPSEEK_ENDPOINT=https://api.deepseek.com

# OLLAMA
OLLAMA_ENDPOINT=http://localhost:11434

# GROK
GROK_API_KEY=xai-your-grok-api-key-here

# NOVITA
NOVITA_API_KEY=your-novita-api-key-here

# OPENROUTER
OPENROUTER_API_KEY=sk-or-v1-your-openrouter-api-key-here

# AWS BEDROCK
AWS_ACCESS_KEY_ID=your-aws-access-key-id-here
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key-here
AWS_REGION=us-east-1

# AZURE
AZURE_OPENAI_KEY=your-azure-openai-api-key-here
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com/
AZURE_OPENAI_API_VERSION=2025-01-01-preview

# BROWSER-USE CORE SETTINGS
ANONYMIZED_TELEMETRY=true
BROWSER_USE_CLOUD_SYNC=
